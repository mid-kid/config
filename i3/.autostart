#!/bin/sh
~/.fehbg
picom &
start-pulseaudio-x11
xss-lock -l -- xflock4 -n &
xset s off  # Managed by xfce4-power-manager
xset +fp ~/.local/share/fonts
xset fp rehash
xset r rate 300 50

urxvtd &
volumeicon &
xfce4-power-manager &
wpa_gui -t &
redshift-gtk &
udiskie &
blueman-applet &

telegram-desktop -startintray &
