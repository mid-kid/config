"1.2pre-pm-3

loadplugins '\.(js|penta)$'
group user
map -count -modes=n,v <C-h> <C-p>
map -count -modes=n,v <C-l> <C-n>
map -count -modes=n j -builtin <count>4<Down>
map -count -modes=n J -builtin <count><Down>
map -count -modes=n k -builtin <count>4<Up>
map -count -modes=n K -builtin <count><Up>
set cdpath=
set complete=location
set suggestengines=
set defsearch=duckduckgo

" vim: set ft=pentadactyl:
